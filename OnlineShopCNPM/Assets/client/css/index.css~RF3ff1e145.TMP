*{
    box-sizing: border-box;
}
body{
    margin: 0;
    padding: 0;
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}
#space-top{
    width: 100%;
    height: 125px;
}
#header-details{
    position: absolute;
    width: 100%;
    height: 125px;
    left: 0px;
    top: 0px;
    z-index: 2;
}
#header-up{
    float: left;
    width: 100%;
    height: 75px;
    background-color: rgba(255, 255, 255, 0.95);
}
#menu-icon {
    background-image: url('../img/common/icon-menu.png');
    width: 25%;
    height: 100%;
    background-repeat: no-repeat;
    background-size: 25px 25px;
    background-position: 15px 25px;
    padding-left: 48px;
    padding-top: 28px;
    font-size: 11pt;
    cursor: pointer;
    float: left;
    display: none;
    font-weight: bold;
    color: gray;
}
#text-title{
    float: left;
    width: 28%;
    height: 100%;
    padding-top: 10px;
    padding-right: 5px;
    text-align: right;
    font-size: 24pt;
    color: rgb(46, 187, 46);
    font-weight: bolder;
    line-height: 1.6;
}
/* search bar */
#search-area{
    float: left;
    width: 50%;
    height: 100%;
    padding-left: 40px;
    padding-top: 20px;
    padding-right: 10px;
}
#resizing_select{
    width: 50px;
    background-color: rgb(242, 241, 241);
    height: 35px;
    border-radius: 20px;
    outline: 0;
    padding-left: 5px;
    color: grey;
    border: 1px solid rgb(207, 204, 204);
    float: left;
}
#search-bar {
    float: left;
    height: 35px;
    color: grey;
    padding-left: 15px;
    border-top-left-radius: 20px;
    border-bottom-left-radius: 20px;
    outline: 0;
    background-color: rgb(245, 241, 241);
    width: calc(100% - 100px);
    border: 1px solid rgb(207, 204, 204);
    border-right: none;
    margin-left: 5px;
}
#buttonSearch {
    float: left;
    height: 35px;
    background-image: url('../img/common/icon-search.png');
    background-size: 18px 18px;
    background-position : center;
    background-repeat: no-repeat;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    background-color: rgb(245, 241, 241);
    width: 40px;
    border: 1px solid rgb(207, 204, 204);
    cursor: pointer;
    border-left: none;
}
#icon-help {
    float: left;
    width: 22%;
    height: 100%;
    padding-top: 22px;
}
#icon-heart{
    color: rgb(146, 146, 146);
    border-right: 1px solid rgba(158, 156, 156,0.3);    
    height: 32px;
    padding-left: 14px;
    padding-right: 14px;
    padding-top: 7px;
    cursor: pointer;
    transition: color 0.3s;
    float: left;
}
#icon-heart:hover{
    color: red;
}
#icon-user{
    color: rgb(146, 146, 146);
    border-right: 1px solid rgba(158, 156, 156,0.3);
    height: 32px;
    padding-left: 14px;
    padding-right: 14px;
    padding-top: 3px;
    cursor: pointer;
    transition: color 0.3s;
    float: left;
}
#icon-user:hover > i{
    color: rgb(20, 163, 20);
}
#user-box {
    position: absolute;
    right: 5%;
    top: 65px;
    width: 180px;
    height: 200px;
    z-index: 3;
    background-color: white;
    box-shadow: 0px 0px 5px grey;
    border-radius: 8px;
    display: none;
}
    #user-box .name {
        width: 100%;
        height: 40px;
        color: black;
        float: left;
        text-align: center;
        font-size: 13.5pt;
        font-weight: bold;
        line-height: 2.1;
    }
    #user-box .link {
        width: 100%;
        height: 40px;
        color: black;
        text-decoration-line: none;
        border-top: 1px solid rgba(0,0,0,0.03);
        float: left;
        background-repeat: no-repeat;
        background-size: 20px 20px;
        background-position: 8px 9px;
        padding-left: 35px;
        line-height: 2.9;
    }
        #user-box .link:hover {
            background-color: rgba(0,0,0,0.05);
        }
    #user-box #infor {
        background-image: url('../img/common/user-info-icon.png');
    }
    #user-box #diary {
        background-image: url('../img/common/diary.png');
    }
    #user-box #change-pass {
        background-image: url('../img/common/change-password.png');
    }
    #user-box #logout {
        background-image: url('../img/common/exit.png');
    }
#icon-cart{
    color: rgb(146, 146, 146);
    height: 32px;
    padding-left: 14px;
    padding-top: 7px;
    cursor: pointer;
    transition: color 0.3s;
    float: left;
}
    #icon-cart #amount-display {
        background-color: #39b54a;
        width: auto;
        max-width: 80%;
        border-radius: 7px;
        color: white;
        font-size: 8pt;
        text-align: center;
        margin-top: -15px;
        margin-left: -5px;
    }
    #icon-cart:hover {
        color: black;
    }
#display-product{
    position: absolute;
    width: 300px;
    top: 0px;
    left: 0px;
    background-color: white;
    padding-top: 25px;
}
#display-product .item{
    width: 84%;
    margin-left: 8%;
    max-height: 200px;
    overflow-y: scroll;
}
#display-product .item .img-item{
    width: 60px;
    height: 60px;
    float: left;
}
#display-product .item .item-name{
    color : #246ce0;
}

#header-down {
    float: left;
    width: 100%;
    height: 50px;
    background-color: rgb(96, 96, 255);
    color: white;
    font-size: 13pt;
    font-weight: 500;
}
    #header-down #main {
        margin-left: calc(30% - 252px);
        height: 100%;
        background-color: rgb(46, 187, 46);
