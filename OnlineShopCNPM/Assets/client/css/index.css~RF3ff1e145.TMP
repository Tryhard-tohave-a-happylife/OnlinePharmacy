    }
    #header-down #some1 {
        height: 100%;
        padding: 12px;
        border-radius: 25px;
        float: left;
        color: white;
        text-decoration-line: none;
    }
    #header-down #some2 {
        height: 100%;
        padding: 12px;
        border-radius: 25px;
        float: left;
        color: white;
    }
    #header-down #some3 {
        height: 100%;
        padding: 12px;
        border-radius: 25px;
        float: left;
    }
    #header-down #some4 {
        height: 100%;
        padding: 12px;
        border-radius: 25px;
        float: left;
    }

#contact {
    list-style-type: none;
    float: right;
    margin-right: 30px;
}

    #contact li {
        float: left;
        height: 100%;
        padding: 12px;
        border-radius: 25px;
    }

        #contact li a {
            text-decoration: none;
            color: white;
            font-size: 13pt;
            font-weight: 600;
        }

#test_p {
    width: 400px;
    background-color: black;
    position: absolute;
    font-size: 12pt;
    border-radius: 5px;
    color: white;
    opacity: 0;
    height: 35px;
    margin-left: -135px;
    transition: all 0.3s;
    font-weight: lighter;
    padding-left: 28px;
    line-height: 2.1;
}
#test_a:hover > #test_p {
    opacity: 1;
    transform: translateY(10px);
}
#header-second-main {
    float: left;
    width: 100%;
    height: 300px;
    background-color: #889AB4;
    cursor: grab;
}
#text_header_second {
    width: 60%;
    height: 230px;
    background-color: #889AB4;
    position: absolute;
    margin-top: 10px;
    margin-left: 20%;
    margin-right: 20%;
}

#cho_y_te_xanh {
    font-size: 24px;
    font-family: Rockwell, Times, serif,sans-serif;
    color: white;
    text-align: center;
    margin-top: 10px;
    margin-bottom: 10px;
    width: 100%;
}

#cho_y_te_online {
    font-size: 30px;
    font-weight: 700;
    font-family: 'Times New Roman',sans-serif;
    color: white;
    text-align: center;
    margin-top: 10px;
    margin-bottom: 10px;
    width: 100%;
}

#text_introction {
    color: white;
    font-family: 'Times New Roman',sans-serif;
    font-size: 20px;
    text-align: center;
    width: 100%;
}

#mua_ngay {
    cursor: pointer;
    width: 100%;
    color: black;
    margin-top: 30px;
    text-align: center;
}
/*end of header down*/
.img-adv{
    width: 60px;
    height: 63px; 
    mix-blend-mode: multiply;
}
.adv-inf-first{
    font-size: 1.23em;
    font-weight: 700;
    font-family: sans-serif;
}
#row-adv{
    width: 100%;
}
.adv-inf-second{
    font-family: sans-serif;
    font-size: 13pt;
}
#space-left{
    float: left;
    width: 10%;
    height: 100px;
}
#adv-first{
    float: left;
    width: 20%;
    text-align: center;
    padding-top: 35px;
}
#adv-second{
    float: left;
    width: 20%;
    text-align: center;
    padding-top: 35px;
}
#adv-third{
    float: left;
    width: 20%;
    text-align: center;
    padding-top: 35px;
}
#adv-fourth{
    float: left;
