}
@media screen and (max-width: 1070px){
    #adv-first{
        margin-left: 0;
        width: 25%;
    }
    #adv-second{
        margin-left: 0;
        width: 25%;
    }
    #adv-third{
        margin-right: 0;
        width: 25%;
    }
    #adv-fourth{
        margin-right: 0;
        width: 25%;
    }
    #space-left{
        width: 0%;
    }
}
@media screen and (max-width: 860px){
    #header-down{
        display: none;
    }
    #menu-icon{
        display: block;
    }
    #search-area{
        display: none;
    }
    #text-title{
        width: 50%;
        text-align: center;
    }
    #icon-help{
        width: 25%;
    }
    #header-up{
        border-bottom: 1px solid rgba(163, 163, 163, 0.5);
        box-shadow: 0px 0px 10px grey;
    }
    #space-top{
        height: 75px;
    }
    #icon-heart{
        padding-right: 0px;
        border: none;
    }
    #icon-user{
        padding-right: 0px;
        border: none;
    }
    #icon-contain{
        float: right;
        margin-right: 6px;
    }
}
@media screen and (max-width: 550px){
    #adv-first{
        width: 100%;
    }
    #adv-second{
        width: 100%;
    }
    #adv-third{
        width: 100%;
    }
    #adv-fourth{
        width: 100%;
    }
    #text-menu-icon{
        display: none;
    }
   
}
